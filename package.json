      "@typescript-eslint/parser": "^4.8.1",
      "eslint": "^7.14.0",
      "eslint-config-prettier": "^6.15.0",
      "nodemon": "^2.0.6",
      "prettier": "^2.2.0",
      "ts-node": "^9.0.0",
      "typescript": "^4.1.2"
   },
   "dependencies": {
      "apollo-server-express": "^2.19.0",
